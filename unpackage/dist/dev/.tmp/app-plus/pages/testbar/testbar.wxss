
.tsetbar {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	overflow: hidden;
	height: 100%;
}
.tsetbar{
	height: 1200rpx;
}
.tsetbar .swiper-box {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	width: 100%;
	height: calc(100% - 100rpx);
}
.tsetbar .swiper-tab-list{
	display: inline-block;
	width: 150rpx;
	text-align: center;
}
.fg{
	width: 100%;
	height: 3000rpx;
	background-color: #FE877B;
}
.active{
	color: #007AFF;
}
.tsetbar .list{
	width: 750rpx;
